<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net5.0</TargetFrameworks>
    <Version>2021.9.20.1</Version>
    <Authors>Marco Mastropaolo, Blake Pell</Authors>
    <Company>Marco Mastropaolo, Blake Pell</Company>
    <Description>Unofficial Moonsharp Lua interpreter forked and targetted .NET 5.</Description>
    <PackageTags>moonsharp lua uwp netcoreapp</PackageTags>
    <Copyright>2020-2021</Copyright>
    <AssemblyVersion>2021.9.20.1</AssemblyVersion>
    <FileVersion>2021.9.20.1</FileVersion>
    <PackageId>MoonSharp.Core</PackageId>
    <Product>MoonSharp (Unofficial)</Product>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyName>MoonSharp.Interpreter</AssemblyName>
    <RootNamespace>MoonSharp.Interpreter</RootNamespace>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/blakepell/Moonsharp</PackageProjectUrl>
    <RepositoryUrl>https://github.com/blakepell/Moonsharp</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageReleaseNotes>Project will now target only the framework required by Avalon.</PackageReleaseNotes>
  </PropertyGroup>
  <ItemGroup>
    <None Include="LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="ZString" Version="2.4.2" />
  </ItemGroup>
</Project>
